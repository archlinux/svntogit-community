+
+func (self *SearchInstalledItemTransactionTestSuite) TestSearchInstalledItemTransactionCancel(c *C.C) {
+	self.testSearchInstalledItemTransaction(c, "fire", func(res []Result, c *C.C) {
+		c.Assert(len(res), C.Equals, 0)
+	}, time.Second, true)
+}
