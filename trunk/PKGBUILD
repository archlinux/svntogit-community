pkgname=processing-examples
pkgver=3.3.1
pkgrel=1
arch=('any')
pkgdesc='Examples for Processing'
url='https://www.processing.org/'
license=('GPL' 'LGPL')
makedepends=('git')
source=("git+https://github.com/processing/processing-docs#commit=8da681de9748dcf64969ea37c3104402df394b09")
